@model MvcApplication1.DbModels.Projects
@{
    ViewBag.Title = "Configure";


}

    <div class="container" id="main">
        <div class="flash affix-top"></div>

       
        <!--Navigation bar-->
        @Html.Partial("_MainTabMenu")
        <!--Builds-->
        <div class="heading-split span12">
            <h2>Builds</h2>
        </div>

        <div class=" span4" id="project-builds">
            <div class="quick-build">
                <ul class="quick-build-options box">
                    <li>
                        <label for="quick-build-pdf" class="quick-build-choice">
                            <input id="quick-build-pdf" type="checkbox" name="formats[]" value="pdf"> PDF
                        </label>
                    </li>
                    <li>
                        <label for="quick-build-epub" class="quick-build-choice">
                            <input id="quick-build-epub" type="checkbox" name="formats[]" value="epub"> EPUB
                        </label>
                    </li>
                    <li>
                        <label for="quick-build-mobi" class="quick-build-choice">
                            <input id="quick-build-mobi" type="checkbox" name="formats[]" value="mobi"> MOBI
                        </label>
                    </li>
                    <li>
                        <label for="quick-build-html" class="quick-build-choice">
                            <input id="quick-build-html" type="checkbox" name="formats[]" value="html"> HTML
                        </label>
                    </li>
                </ul><a class="down10 block align-center btn btn-medium action  default btn" href="#">Build!</a>
            </div>
            <div class="build-list down10"> </div>
             <a class="btn block align-center down10 align-right" href="/demo_book/my-new-project/builds">All builds</a>
        </div>

        <div id="recent-builds" class=" span4">
            <div class="build-list ">
                <div class="list-item">
                    <div class="build box ">
                        <div class="box-top">
                            <img class="build-user-image" src="https://secure.gravatar.com/avatar/b5a4cc2539e3d81d9944eaf07e3163eb?s=80" title="demo_book">
                            <span class="build-branch"><i class="glyphicons git_branch"></i><span class="build-branch-name">demo_book</span></span> <span title="January 15 2015 6:34 PM" class="pull-right">a few seconds ago</span>
                        </div>
                        <ul class="list">

                            <li class="build-format list-item" data-index="">
                                <i class="icn-round icn-success glyphicons ok_2"></i>
                                <span class="format">html</span>
                                <span class="status completed">

                                    <a href="http://orm-atlas2-prod.s3.amazonaws.com/html/e2ada6ebed0547017a708dd0d49b4cac.zip" target="_blank" class="pull-right">Download</a>

                                </span>
                            </li>

                        </ul>

                        <div class="box-bottom">
                            <a href="/demo_book/my-new-project/builds/31136" class="build-log-btn">Build Log</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>


    </div>

